<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginPageController">
   <children>
      <TableView fx:id="customerTable" layoutX="167.0" layoutY="55.0" prefHeight="273.0" prefWidth="566.0">
        <columns>
          <TableColumn fx:id="customerIDColumn" prefWidth="90.0" text="Customer ID" />
          <TableColumn fx:id="customeNameColumn" prefWidth="81.0" text="Name" />
            <TableColumn fx:id="customerAddressColumn" prefWidth="76.0" text="Address" />
            <TableColumn fx:id="customerZipColumn" prefWidth="85.0" text="Zip Code" />
            <TableColumn fx:id="customerPhoneColumn" prefWidth="119.0" text="Phone Number" />
            <TableColumn fx:id="customerDivisionColumn" prefWidth="112.0" text="Division Number" />
        </columns>
      </TableView>
      <ButtonBar layoutX="201.0" layoutY="328.0" prefHeight="40.0" prefWidth="499.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#onAddAppointment" text="Add Appointment" />
            <Button mnemonicParsing="false" onAction="#onAddCustomer" text="Add Customer" />
            <Button mnemonicParsing="false" onAction="#onUpdateCustomer" text="Update Customer" />
            <Button mnemonicParsing="false" onAction="#onDeleteCustomer" text="Delete Customer" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
